{
  "manifest_version": 2,

  "name": "myfeeds",
  "description": "Add links to personal RSS feeds",
  "version": "1.0",
  "background": {
    "scripts": ["assets/chromext.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "popup.html",
  "permissions": [
    "contextMenus",
    "http://192.168.99.100:3456/api/v1",
    "http://127.0.0.1:3456/api/v1",
    "http://api.myfeeds.polymatix.svc.tutum.io/api/v1"
  ]
}
